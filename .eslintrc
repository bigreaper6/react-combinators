{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "semi": [2, "never"],
    "quotes": [2, "double"]
  },
  "plugins": ["react"],
  "env": {
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
